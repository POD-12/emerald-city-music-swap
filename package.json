{
  "name": "mern",
  "version": "1.0.0",
  "description": "Mern Demo",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/recordSeed.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "fastest-validator": "^1.10.0",
    "if-env": "^1.0.4",
    "mdbreact": "^5.0.1",
    "mongoose": "^5.12.0",
    "mongoose-seed": "^0.6.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-router-dom": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/POD-12/emerald-city-music-swap.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/POD-12/emerald-city-music-swap/issues"
  },
  "homepage": "https://github.com/POD-12/emerald-city-music-swap#readme"
}
